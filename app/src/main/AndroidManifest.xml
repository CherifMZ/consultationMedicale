<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consultation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_icon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Consultation">
        <activity android:name=".ui.view.activity.PatientMainActivity"></activity>
        <activity android:name=".ui.view.activity.AffichageTreatmentActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.ProfilTreatmentActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.ConseilActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.PriseRdvActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.MainActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.ProfilMedecinActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.MedecinActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.AffichageMedecinActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ui.view.activity.AuthentificationActivity">
            android:screenOrientation="portrait"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>