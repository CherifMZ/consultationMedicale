<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consultation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Consultation">
        <activity android:name=".ui.view.activity.CalendarActivity"></activity>
        <activity android:name=".ui.view.activity.MedecinRdvActivity" />
        <activity android:name=".ui.view.activity.AccueilActivity" />
        <activity android:name=".ui.view.activity.PatientRdvActivity" />
        <activity android:name=".ui.view.activity.PriseRdvActivity" />
        <activity android:name=".ui.view.activity.MainActivity" />
        <activity android:name=".ui.view.activity.ProfilMedecinActivity" />
        <activity android:name=".ui.view.activity.MedecinActivity" />
        <activity android:name=".ui.view.activity.AffichageMedecinActivity" />
        <activity android:name=".ui.view.activity.AuthentificationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>